{
  "ConnectionStrings": {
    "ApiGeneratorDatabase": "Host=host.docker.internal;database=postgres;username=postgres;password=admin;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Api": {
    "Basic": {
      "JsonMode": "local",
      "JsonUri": "./ApiDefinition.json"
    },
    "Swagger": {
      "EnableProduction": "false", // Enable/Disable for production builds
      "Description": "Sample API created by the Net Dynamic API Generator, this API was fully created using a remote JSON definition.",
      "Version": "v1",
      "Title": "TCDev API Generator Sample",
      "ContactMail": "tim@smoower.com",
      "ContactUri": "https://www.smoower.com"
    },
    "Database": {
      "DatabaseType": "Postgres",
      //"DatabaseType": "InMemory",
      "Connection": ""
    },
    "Odata": {
      "Enabled": true
    }
  }
}
